{
  "name": "aovens-coursera-emotion-test-lib",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist"
  },
  "files": [
    "dist/**"
  ],
  "dependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-react": "7.12.1",
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/react": "^11.1.4",
    "react": "^18.0.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "edge": "17",
            "firefox": "60",
            "chrome": "67",
            "safari": "11.1"
          },
          "useBuiltIns": "usage",
          "corejs": "3.6.5"
        }
      ],
      [
        "@babel/preset-react",
        {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      ]
    ],
    "plugins": [
      "@emotion/babel-plugin"
    ]
  }
}
